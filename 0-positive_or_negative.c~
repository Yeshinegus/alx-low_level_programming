#include <stdio.h>
#include <time.h>
/**
 *
 * main - entry point
 *
 * Return: always true
 *
 */
int main(void)
{
int n;

srand(time(0));

n = rand() - RAND_MAX() / 2;
if (n > 0)
  printf("is positive\n");
else if(n < 0)
  printf("is negative\n");
else if (n == 0)
  printf("is zero");
return (0);
}
